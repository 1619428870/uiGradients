{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,POST"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/search/:match*",
      "destination": "/api/search/:match*"
    },
    {
      "source": "/gradients",
      "destination": "/api/all"
    },
    {
      "source": "/random",
      "destination": "/api/random"
    },
    {
      "source": "/reds",
      "destination": "/json/reds.json"
    },
    {
      "source": "/oranges",
      "destination": "/json/oranges.json"
    },
    {
      "source": "/yellows",
      "destination": "/json/yellows.json"
    },
    {
      "source": "/greens",
      "destination": "/json/greens.json"
    },
    {
      "source": "/cyans",
      "destination": "/json/cyans.json"
    },
    {
      "source": "/blues",
      "destination": "/json/blues.json"
    },
    {
      "source": "/magentas",
      "destination": "/json/magentas.json"
    },
    {
      "source": "/whites",
      "destination": "/json/whites.json"
    },
    {
      "source": "/grays",
      "destination": "/json/grays.json"
    },
    {
      "source": "/greys",
      "destination": "/json/grays.json"
    },
    {
      "source": "/blacks",
      "destination": "/json/blacks.json"
    }
  ]
}