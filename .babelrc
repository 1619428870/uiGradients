{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "inline-react-svg",
      {
        "removeAttrs": {
          "attrs": "(data-name)"
        }
      },
      {
        "cleanupIDs": true
      },
      {
        "removeUselessStrokeAndFill": true
      },
      {
        "removeMetadata": true
      },
      {
        "removeTitle": true
      },
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ]
  ]
}
